{
    "extends": [
        "next/core-web-vitals",
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    },
    "plugins": ["react", "prettier"],
    "rules": {
        "react/react-in-jsx-scope": "off",
        "@next/next/no-img-element": "off",
        "react/prop-types": 0
    }
}
